# ローカルワーカー設定ファイル
# このファイルをコピーして .env.worker を作成してください

# Render API URL
RENDER_API_URL=https://create-cut-out-videos.onrender.com

# ワーカーID（任意の名前、複数ワーカーの場合は変更）
WORKER_ID=local-worker-1

# ローカルディレクトリ
DOWNLOAD_DIR=./downloads
OUTPUT_DIR=./output
TEMP_DIR=./temp

# ポーリング間隔（秒）
POLLING_INTERVAL=30

# 並列処理数（通常は1でOK）
MAX_WORKERS=1

# Google Drive設定（オプション - Phase 3で使用）
GOOGLE_DRIVE_ENABLED=false
GOOGLE_DRIVE_FOLDER_ID=

# ログレベル
LOG_LEVEL=INFO
